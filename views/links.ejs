<%- include('partials/_header') %>
    <div class="links-content">
        <h3>Links Data</h3>
        <div class="tableContainer">
            <table>
                <tr>
                    <th>Link</th>
                    <th>Created At</th>
                </tr>
    
                <% links.forEach(link=> { %>
                    <tr>
                        <td><a href="<%= link.url %>">
                                <%= link.url %>
                            </a></td>
                        <td>
                            <%= link.created_at.toLocaleTimeString() %>
                            <%= link.created_at.toDateString() %>
                        </td>
                    </tr>
                    <% }); %>
            </table>
        </div>
        
    </div>
    <%- include('partials/_footer') %>